c-----------------------------------------------------------------------
      subroutine useric(ix,iy,iz,eg) ! set up initial conditions

c     implicit none

      integer ix,iy,iz,eg

      include 'SIZE'
      include 'TOTAL'
      include 'NEKUSE'

      real htwo, kx, ky

      XLEN = 400.0
      YLEN = 200.0

      htwo = 2*uparam(3)

      eps = 2e-2
      kx = 40
      ky = 20

      alpha = kx * 400/XLEN
      beta  = ky * 200/YLEN

      ux = 6.5*log(1+z)/log(1+htwo)
      uy = 0.0
      uz = 0.0

      ux = ux + eps*beta*sin(alpha*x)*cos(beta*y)
      uy =    -eps*alpha*cos(alpha*x)*sin(beta*y)
      uz =      eps*beta*sin(alpha*x)*cos(beta*y)

      return
      end

